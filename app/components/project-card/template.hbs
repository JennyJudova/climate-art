<div class="project-card">
  <img class="object-fit-contain" src={{@project.image}} alt="Girl in a jacket">
  <div class="info">
    <h2>{{@project.title}}</h2>
    <p>{{@project.description}}</p>
    <p>{{@project.location}}</p>
    <ul class="no-bullets">
      {{#each @project.partners as |partner|}}
      <li class="horizontal-list">
        <img src={{partner}} alt={{partner}}>
      </li>
      {{/each}}
    </ul>
  </div>
</div>